{
    "all": true,
    "check-coverage": true,
    "exclude": ["**/*.cy.{js,jsx,ts,tsx}"],
    "reporter": ["cobertura", "html", "text"],
    "report-dir": "./build/reports",
    "temp-dir": "./build/.nyc_output",
    "extension":[".js", ".cjs", ".mjs", ".ts", ".tsx", ".jsx"],
    "include": ["src/components/**/*.{js,jsx,ts,tsx}"],
    "branches": 75,
    "lines": 75,
    "functions": 75,
    "statements": 75
}
